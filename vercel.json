{
    "version": 2,
    "name": "ams",
    "builds": [
        {
            "src": "./server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./server/index.js"
        }
    ],
    "functions": {
        "./server/router/**/*.js": {
          "runtime": "nodejs14.x",
          "maxDuration": 10
    }}    
}